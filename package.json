    "@types/md5": "^2.3.5",
    "@types/seedrandom": "^3.0.8",
    "@types/webfontloader": "^1.6.35",
    "autoprefixer": "^10.4.7",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.14",
    "postcss-load-config": "^4.0.1",
    "svelte": "^4.2.1",
    "svelte-awesome-color-picker": "^3.1.0",
    "svelte-check": "^3.5.2",
    "svelte-preprocess": "^5.0.4",
    "tailwindcss": "^3.1.5",
    "tslib": "^2.5.0",
    "typescript": "^5.6.3",
    "vite": "^4.2.0"
  },
  "dependencies": {
    "@diffusion-studio/ffmpeg-js": "^0.2.3",
    "@neodrag/svelte": "^2.0.3",
    "@sentry/svelte": "^7.74.0",
    "@splidejs/svelte-splide": "^0.2.9",
    "@svelte-web-fonts/google": "^1.0.4",
    "@types/color-rgba": "^2.1.3",
    "@types/sortablejs": "^1.15.8",
    "@types/uuid": "^9.0.7",
    "@xenova/transformers": "^2.16.0",
    "@xyflow/svelte": "^0.0.26",
    "ag-psd": "^19.0.0",
    "cbor": "^9.0.2",
