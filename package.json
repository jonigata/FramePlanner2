{
  "name": "frameplanner",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev-https": "vite --config vite.https.config.ts",
    "build": "npm run stamp && vite build",
    "devbuild": "vite build --mode development",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.svelte-check.json",
    "deploy": "git branch --show-current | grep -q '^sns_main$' && npm run stamp && wrangler pages deploy || echo 'Deployment failed: not on sns_main branch'",
    "which": "which wrangler",
    "vitest": "vitest",
    "deploy:preview": "firebase hosting:channel:deploy preview",
    "list-preview": "firebase hosting:channel:list",
    "stamp": "git log -10 | tee public/stamp.txt",
    "branches": "git for-each-ref --sort=-committerdate refs/heads/ --format='%(committerdate:short) %(refname:short)'",
    "merged": "git branch --merged",
    "merged:delete": "git branch --merged | grep -v '^\\*' | xargs -n 1 git branch -d",
    "serve": "supabase functions serve --debug --env-file supabase/functions/.env.local --no-verify-jwt",
    "released-version": "curl https://frameplanner.manga-farm.online/stamp.txt",
    "webp": "sh convert_to_webp.sh"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241205.0",
    "@jridgewell/trace-mapping": "^0.3.25",
    "@skeletonlabs/skeleton": "^1.12.0",
    "@sveltejs/vite-plugin-svelte": "^2.5.3",
    "@tsconfig/svelte": "^5.0.4",
    "@types/file-saver": "^2.0.7",
    "@types/md5": "^2.3.5",
    "@types/node": "^22.10.5",
    "@types/seedrandom": "^3.0.8",
    "@types/webfontloader": "^1.6.35",
    "autoprefixer": "^10.4.7",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.14",
    "postcss-load-config": "^4.0.1",
    "svelte": "^4.2.1",
    "svelte-awesome-color-picker": "^3.1.0",
    "svelte-check": "^4.1.1",
    "svelte-preprocess": "^5.0.4",
    "tailwindcss": "^3.1.5",
    "tslib": "^2.5.0",
    "typescript": "^5.7.2",
    "vite": "^4.5.14",
    "vite-node": "^3.1.3",
    "vitest": "^3.1.3"
  },
