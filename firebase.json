{
  "hosting": {
    "public": "dist",
    "headers": [ {
      "regex": "/.*",
      "headers" : [
        {
          "key" : "Cross-Origin-Opener-Policy",
          "value" : "same-origin"
        },
        {
          "key" : "Cross-Origin-Embedder-Policy",
          "value" : "require-corp"
        }
      ]
    }],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}
